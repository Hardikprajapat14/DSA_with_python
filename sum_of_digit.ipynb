        "#using logic\n",
        "def sum_of_digit(n):\n",
        "  s = 0\n",
        "  while n > 0:\n",
        "    s+= n % 10\n",
        "    n//= 10\n",
        "  return s\n",
        "\n",
        "num = 666\n",
        "print(sum_of_digits(num))"
