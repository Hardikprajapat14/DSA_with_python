      "source": [
        "#generator function allows you to declare a function that behaves like an iterator, providing a faster and easier way to create iterators\n",
        "def count_up_to(n):\n",
        "  count = 1\n",
        "  while count <= n:\n",
        "    yield count\n",
        "    count += 1\n",
        "\n",
        "gen = count_up_to(3)\n",
        "for i in gen:\n",
        "  print(i)"
      ]
